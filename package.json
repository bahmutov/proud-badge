{
  "name": "proud-badge",
  "version": "0.0.4",
  "description": "Badge generation for NPM downloads by author",
  "main": "index.js",
  "scripts": {
    "test": "node node_modules/gt test/*.js --output"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bahmutov/proud-badge.git"
  },
  "keywords": [
    "npm",
    "npm-stats",
    "proud",
    "badge",
    "downloads"
  ],
  "pre-commit": "grunt",
  "pre-push": [
    "rm -rf node_modules",
    "npm install",
    "grunt",
    "npm test"
  ],
  "author": "Gleb Bahmutov @bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/proud-badge/issues"
  },
  "dependencies": {
    "proud": "0.0.5",
    "q": "~0.9.7",
    "check-types": "~1.1.0",
    "deferred-exec": "~0.3.1",
    "humanize-plus": "~1.4.2"
  },
  "devDependencies": {
    "jshint-stylish": "~0.1.3",
    "grunt-complexity": "~0.1.3",
    "grunt-contrib-jshint": "~0.7.2",
    "matchdep": "~0.3.0",
    "grunt": "~0.4.1",
    "grunt-readme": "~0.3.0",
    "pre-git": "0.0.13",
    "gt": "~0.8.20"
  }
